meh - t : /Applications/Alacritty.app/Contents/MacOS/alacritty

ctrl + alt - h : yabai -m window --focus west
ctrl + alt - j : yabai -m window --focus south
ctrl + alt - k : yabai -m window --focus north
ctrl + alt - l : yabai -m window --focus east

ctrl + alt - 1 : yabai -m window --display 1 #yabai -m display --focus 1 # requires scripting
ctrl + alt - 2 : yabai -m window --display 2 #yabai -m display --focus 2 # requires scripting

fn + ctrl - d : $(yabai -m window $(yabai -m query --windows --window | jq -re ".id") --close)
fn + ctrl - f : yabai -m window --toggle zoom-fullscreen
fn + ctrl - r : yabai -m space --rotate 90
fn + ctrl - t : yabai -m window --toggle float; yabai -m window --grid 4:4:1:1:2:2
fn + ctrl - e : yabai -m window --toggle split
fn + ctrl - b : yabai -m space --balance

fn + ctrl - h : yabai -m window --swap west
fn + ctrl - j : yabai -m window --swap south
fn + ctrl - k : yabai -m window --swap north
fn + ctrl - l : yabai -m window --swap east

fn + ctrl - y : yabai -m window --resize right:-20:0
fn + ctrl - o : yabai -m window --resize left:20:0
fn + ctrl - u : yabai -m window --resize top:0:20
fn + ctrl - i : yabai -m window --resize bottom:0:-20


